create database Loan;
use Loan;
drop table Loan;
create table customer(customerID int constraint pk_customer_ID primary key,Name varchar(20),EmailAddress varchar(40) constraint uq_email unique,Phone_Number varchar(10),Address varchar(50),creditScore decimal);
create table Loan(loanID int constraint pk_loanID primary key,customerID int constraint fk_customer_ID foreign key references customer(customerID) on delete cascade,principleAmount decimal,interestRate decimal,loanTerm decimal,loanType varchar(20),loanStatusÂ varchar(20),PropertyAddress varchar(30),propertyValue decimal,carModel varchar(20),carValue decimal);

select * from loan;
